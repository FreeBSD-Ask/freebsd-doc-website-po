"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>RELENG_4_9</tt> branch</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">18 Oct 2003</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">22 Oct 2003</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">The release branch is created.</td>\n"
"  </tr>\n"
msgstr ""

