"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Third release candidate</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">17 Oct 2003</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">18 Oct 2003</td>\n"
"    <td rowspan=\"1\" colspan=\"1\"></td>\n"
"  </tr>\n"
msgstr ""

