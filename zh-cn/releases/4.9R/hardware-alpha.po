#. type: delimited block + 4
#: content/en/releases/4.9R/hardware-alpha.adoc:2561
#, no-wrap
msgid "<p>DS20 needs</p>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.9R/hardware-alpha.adoc:2565
#: content/en/releases/4.9R/hardware-alpha.adoc:2798
#, no-wrap
