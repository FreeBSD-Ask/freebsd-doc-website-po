#, no-wrap
msgid ""
"                  <td width=\"83%\" align=\"LEFT\" valign=\"TOP\">The\n"
"                  starting I/O port used by the device, shown in\n"
"                  hexadecimal.</td>\n"
"                </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.4R/hardware-i386.adoc:3511
#, no-wrap
msgid ""
"                <tr>\n"
"                  <td width=\"17%\" align=\"LEFT\" valign=\"TOP\">\n"
"                  IRQ</td>\n"
msgstr ""

