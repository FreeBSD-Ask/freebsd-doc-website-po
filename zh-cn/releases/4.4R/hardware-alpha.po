#. type: delimited block + 4
#: content/en/releases/4.4R/hardware-alpha.adoc:3958
#, no-wrap
msgid ""
"              <li>\n"
"                <p>embedded dual Intel 82559 10/100Mbit\n"
"                Ethernet</p>\n"
"              </li>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.4R/hardware-alpha.adoc:3963
#, no-wrap
msgid ""
