#. type: delimited block + 4
#: content/en/releases/4.4R/hardware-alpha.adoc:3922
#, no-wrap
msgid ""
"              <li>\n"
"                <p>memory bus: 100MHz 256-bit wide</p>\n"
"              </li>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.4R/hardware-alpha.adoc:3926
#, no-wrap
msgid ""
