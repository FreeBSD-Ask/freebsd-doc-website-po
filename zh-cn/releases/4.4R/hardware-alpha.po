#: content/en/releases/4.4R/hardware-alpha.adoc:3791
#: content/en/releases/4.4R/hardware-alpha.adoc:3912
#, no-wrap
msgid "            <p>Features:</p>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.4R/hardware-alpha.adoc:386
#, no-wrap
msgid ""
"            <ul>\n"
"              <li>\n"
"                <p>21066 Alpha CPU at 166 MHz or 21066A CPU at\n"
"                233MHz. 21068 CPUs are also possible, but are even\n"
"                slower.</p>\n"
"              </li>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.4R/hardware-alpha.adoc:391
#, no-wrap
msgid ""
"              <li>\n"
"                <p>on-board Bcache / L2 cache: 0, 256k or 1 Mbyte\n"
"                (uses DIL chips)</p>\n"
"              </li>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.4R/hardware-alpha.adoc:396
#, no-wrap
msgid ""
"              <li>\n"
"                <p>PS/2 mouse &amp; keyboard port OR 5pin DIN\n"
"                keyboard (2 mainboard models)</p>\n"
"              </li>\n"
msgstr ""

#. type: delimited block + 4
