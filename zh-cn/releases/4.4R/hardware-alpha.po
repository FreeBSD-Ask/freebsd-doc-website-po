"                  <li>\n"
"                    <p>uses parity memory</p>\n"
"                  </li>\n"
"                </ul>\n"
"                <br>\n"
"                <br>\n"
"              </li>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.4R/hardware-alpha.adoc:1641
#, no-wrap
