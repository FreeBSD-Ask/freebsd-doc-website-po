msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.8R/hardware-alpha.adoc:3028
#, no-wrap
msgid ""
"            <ul>\n"
"              <li>\n"
"                <p>21264 EV6 CPU at 500 or 670 MHz</p>\n"
"              </li>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.8R/hardware-alpha.adoc:3032
#, no-wrap
msgid ""
"              <li>\n"
"                <p>dual CPU capable machine</p>\n"
"              </li>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.8R/hardware-alpha.adoc:3036
#: content/en/releases/4.8R/hardware-alpha.adoc:3198
#, no-wrap
msgid ""
"              <li>\n"
"                <p>L2 / Bcache: 4 Mbytes per CPU</p>\n"
"              </li>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.8R/hardware-alpha.adoc:3041
#, no-wrap
msgid ""
"              <li>\n"
"                <p>memory bus: dual 256 bit wide with crossbar\n"
