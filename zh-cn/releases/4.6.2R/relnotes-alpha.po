#: content/en/releases/4.6.2R/relnotes-alpha.adoc:390
#, no-wrap
msgid ""
"            <h3 class=\"SECT3\"><a name=\"AEN177\">2.1.8 Multimedia\n"
"            Support</a></h3>\n"
"          </div>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6.2R/relnotes-alpha.adoc:396
#, no-wrap
msgid ""
"            <h3 class=\"SECT3\"><a name=\"AEN186\">2.1.9 Contributed\n"
"            Software</a></h3>\n"
msgstr ""

