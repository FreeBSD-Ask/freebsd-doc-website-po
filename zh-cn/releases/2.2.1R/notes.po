msgid "HP PC Lan+ cards (model numbers: 27247B and 27252A).\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:303
#, no-wrap
msgid ""
"Intel EtherExpress (not recommended due to driver instability)\n"
"Intel EtherExpress Pro/10\n"
"Intel EtherExpress Pro/100B PCI Fast Ethernet\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:306
#, no-wrap
msgid ""
"Isolan AT 4141-0 (16 bit)\n"
"Isolink 4110     (8 bit)\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:308
#, no-wrap
msgid "Novell NE1000, NE2000, and NE2100 ethernet interface.\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:310
#, no-wrap
msgid "3Com 3C501 cards\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:312
#, no-wrap
msgid "3Com 3C503 Etherlink II\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:314
#, no-wrap
msgid "3Com 3c505 Etherlink/+\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:316
#, no-wrap
msgid "3Com 3C507 Etherlink 16/TP\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:319
#, no-wrap
msgid ""
"3Com 3C509, 3C579, 3C589 (PCMCIA), 3C590/592/595/900/905 PCI and EISA\n"
"(Fast) Etherlink III / (Fast) Etherlink XL\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:321
#, no-wrap
msgid "Toshiba ethernet cards\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:324
#, no-wrap
msgid ""
"PCMCIA ethernet cards from IBM and National Semiconductor are also\n"
"supported.\n"
msgstr ""

#. type: delimited block . 4
#: content/en/releases/2.2.1R/notes.adoc:328
#, no-wrap
