#: content/en/releases/4.7R/schedule.adoc:222
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">src tree tagged.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">29 Sep 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">06 Oct 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>RELENG_4_7_0_RELEASE</tt> tag for "
"src.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.7R/schedule.adoc:229
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Final builds.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">08 Oct 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">08 Oct 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Final builds for x86 and Alpha in a "
"pristine environment.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.7R/schedule.adoc:239
#, no-wrap
