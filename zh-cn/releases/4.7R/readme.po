#. type: delimited block + 4
#: content/en/releases/4.7R/readme.adoc:37
#, no-wrap
msgid "        <h1 class=\"SECT1\"><a name=\"AEN13\">1 Introduction</a></h1>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.7R/readme.adoc:40
#, no-wrap
msgid ""
"        <p>This distribution is a release of FreeBSD 4.7-RELEASE,\n"
"        the latest point along the 4-STABLE branch.</p>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.7R/readme.adoc:43
#: content/en/releases/4.7R/readme.adoc:81
#: content/en/releases/4.7R/readme.adoc:105
