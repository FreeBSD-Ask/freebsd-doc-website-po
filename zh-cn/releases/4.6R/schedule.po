#. type: YAML Front Matter: title
#: content/en/releases/4.6R/schedule.adoc:1
#, no-wrap
msgid "FreeBSD 4.6 Release Process"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:10
#, no-wrap
msgid "<h1>Introduction</h1>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:15
#, no-wrap
msgid ""
"<p>This is a specific schedule for the release of FreeBSD 4.6.  For\n"
"  more general information about the release engineering process,\n"
"  please see the <a href=\"../../../releng/index.html\" "
"shape=\"rect\">Release\n"
"  Engineering</a> section of the web site.</p>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:20
#, no-wrap
msgid ""
"<p>General discussions about the release engineering process or\n"
"  quality assurance issues should be sent to the public <a "
"href=\"mailto:FreeBSD-qa@FreeBSD.org\" shape=\"rect\">FreeBSD-qa</a> mailing "
"list.\n"
"  <a href=\"../../../doc/en_US.ISO8859-1/books/faq/misc.html#DEFINE-MFC\" "
"shape=\"rect\">MFC</a>\n"
"  requests should be sent to <a href=\"mailto:re@FreeBSD.org\" "
"shape=\"rect\">re@FreeBSD.org</a>.</p>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:22
#, no-wrap
msgid "<h1>Schedule</h1>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:70
#, no-wrap
msgid ""
"<table class=\"tblbasic\">\n"
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Action</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Expected</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Actual</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Description</td>\n"
"  </tr>\n"
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Reminder announcement</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">01 Apr 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">09 Apr 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Release Engineers send announcement "
"email to\n"
"      <tt>developers@FreeBSD.org</tt> with a rough schedule for the FreeBSD "
"4.6\n"
"      release.</td>\n"
"  </tr>\n"
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">4.6-PRERELEASE Testing Guide "
"published</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Ongoing</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">--</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">A <a "
"href=\"../../../releases/4.6R/qa.html\" shape=\"rect\">testing guide</a>\n"
"      should be published with information about recent changes and\n"
"      areas of the system that should be thoroughly tested during the\n"
"      pre-release/RC period.</td>\n"
"  </tr>\n"
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>4.6-PRERELEASE</tt></td>\n"
"    <td rowspan=\"1\" colspan=\"1\">01 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">01 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>newvers.sh</tt>, and "
"<tt>release.ent</tt> updated.</td>\n"
"  </tr>\n"
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">FTP site updated</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">01 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">18 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>pkg_add -r</tt> works for "
"<tt>4.6-PRERELEASE</tt> machines.</td>\n"
"  </tr>\n"
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Code freeze begins</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">01 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">01 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">After this date, all commits to the "
"<tt>RELENG_4</tt> branch\n"
"      must be approved by <a href=\"mailto:re@FreeBSD.org\" "
"shape=\"rect\">re@FreeBSD.org</a>.  Certain highly\n"
"      active documentation committers are exempt from this rule for\n"
"      routine man page / release note updates.  Heads-up emails\n"
"      should be sent to the <tt>developers@</tt>, <tt>stable@</tt>\n"
"      and <tt>qa@</tt> lists.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:78
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Commercial software demos "
"updated.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">15 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">--</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">The second disc contains commercial "
"software demos, these\n"
"      demos should be updated to contain the latest versions.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:85
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>4.6-RC</tt></td>\n"
"    <td rowspan=\"1\" colspan=\"1\">15 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">15 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>newvers.sh</tt> and "
"<tt>release.ent</tt> updated.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:94
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Announce the Ports Freeze</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">15 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">19 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Someone from <tt>portmgr</tt> should "
"email\n"
"      <tt>freebsd-ports@</tt> and BCC: <tt>developers@</tt> to set a date\n"
"      for the week long ports freeze and tagging of the ports tree.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:104
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Heads up to hubs</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">28 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">--</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">A message should be sent to\n"
"      <tt>freebsd-hubs@FreeBSD.org</tt> before the release is uploaded.  "
"The\n"
"      site administrators have frequently requested advance notice for\n"
"      new ISOs.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:118
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">First release candidate</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">16 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">17 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">The first release candidate for the x86 "
"and Alpha architecture is\n"
"      released.  ISO images should be uploaded to\n"
"      <tt>ftp-master.FreeBSD.org</tt> and\n"
"      <tt>releng4.FreeBSD.org</tt>.  A network install directory should\n"
"      be uploaded to <tt>ftp-master.FreeBSD.org</tt>.  The\n"
"      <tt>packages/</tt> and <tt>XF86336/</tt> directories may be\n"
"      symlinked to save space, as long as you're sure to use\n"
"      <em>relative</em> symlinks.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:126
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Heads up to <tt>-stable</tt></td>\n"
"    <td rowspan=\"1\" colspan=\"1\">17 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">17 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">A message should be sent to "
"<tt>qa@FreeBSD.org</tt> and\n"
"      <tt>stable@FreeBSD.org</tt> after the snapshot is uploaded.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:134
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Package split posted</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">17 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">--</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">The proposed package split (which "
"packages go on which disc of\n"
"      the 4 CD set) should be posted to <tt>qa@FreeBSD.org</tt>,\n"
"      <tt>ports@FreeBSD.org</tt>, and <tt>stable@FreeBSD.org</tt>.</td>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:136
#, no-wrap
msgid "  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:144
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Second release candidate</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">?? May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">18 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Note: the release date of this candidate "
"depends on the user\n"
"      experience with RC1.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:151
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Third release candidate</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">--</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">--</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">There will be no 4.6 RC3 "
"snapshot.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:162
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Ports tree frozen.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">24 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">25 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>CVSROOT/avail</tt> locks out "
"everyone but the person who\n"
"      does the tagging.  It will take approximately 2 hours to tag the\n"
"      entire ports tree with the <tt>RELEASE_4_6_0</tt> tag.  After\n"
"      this time, the ports/ tree will be re-opened for commits, but\n"
"      commits made after tagging will not go in 4.6-RELEASE.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:170
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Final package build starts</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">25 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">--</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">The ports cluster and <a "
"href=\"http://bento.FreeBSD.org/\" shape=\"rect\">bento</a>\n"
"      build final packages.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:185
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Package split</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">26 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">--</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">The packages must be split so that "
"packages with similar\n"
"      dependencies appear on each of the four discs, with the most\n"
"      popular packages appearing on the first disc.</td>\n"
"  </tr>\n"
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>doc/</tt> tree tagged.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">27 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">27 May 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>RELEASE_4_6_0</tt> tag for "
"docs.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:193
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Fourth release candidate</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">3 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">6 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">4.6 RC4 for the x86 and Alpha "
"architectures will be made\n"
"      available from <tt>ftp.FreeBSD.org</tt> and the mirrors.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:200
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>RELENG_4_6</tt> branch</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">5 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">6 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">The release branch is created.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:211
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Note to freebsd-stable@</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">5 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">6 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">A note should be sent to the "
"<tt>freebsd-stable@</tt> to let\n"
"      over-anxious users know that the tags have been created but the\n"
"      release still isn't ready.  Tags may be slid before the\n"
"      announcement goes out.  Point users to <tt>freebsd-qa@</tt> for\n"
"      details.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:219
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Version numbers bumped.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">5 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">8 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">The files listed <a "
"href=\"../../../doc/en_US.ISO8859-1/articles/releng/article.html#versionbump\" "
"shape=\"rect\">here</a>\n"
"      are updated to reflect the fact that this is FreeBSD 4.6.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:228
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Update man.cgi on the website.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">6 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">3 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Make sure the 4.6 manual pages are being "
"displayed by default\n"
"      for the man-&gt;web gateway.  Also make sure these man pages are\n"
"      pointed to by docs.xml.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:235
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>src</tt> tree tagged.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">10 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">10 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\"><tt>RELENG_4_6_0_RELEASE</tt> tag for "
"src.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:242
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Final builds.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">10 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">--</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Final builds for x86 and Alpha in a "
"pristine environment.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:250
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Warn <tt>hubs@FreeBSD.org</tt></td>\n"
"    <td rowspan=\"1\" colspan=\"1\">10 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">11 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Heads up email to <a "
"href=\"mailto:hubs@FreeBSD.org\" shape=\"rect\">hubs@FreeBSD.org</a> to give "
"admins\n"
"      time to prepare for the load spike to come.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:257
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Upload to <tt>ftp-master</tt>.</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">10 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">14 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">release and packages uploaded to "
"<tt>ftp-master.FreeBSD.org</tt></td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:265
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Announcement</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">10 Jun 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">15 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Announcement sent out after a majority "
"of the mirrors have\n"
"      received the bits.</td>\n"
"  </tr>\n"
msgstr ""

#. type: delimited block + 4
#: content/en/releases/4.6R/schedule.adoc:274
#, no-wrap
msgid ""
"  <tr>\n"
"    <td rowspan=\"1\" colspan=\"1\">Unfreeze the tree</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">10 June 2002</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">16 June</td>\n"
"    <td rowspan=\"1\" colspan=\"1\">Announcement to developers@ explaining "
"that the release is\n"
"      out, and commits to <tt>RELENG_4</tt> no longer require approval.  "
"Also\n"
"      note the policy for commits to the <tt>RELENG_4_6</tt> branch.</td>\n"
"  </tr>\n"
msgstr ""

